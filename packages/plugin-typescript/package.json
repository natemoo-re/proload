    "@swc-node/register": "^1.3.7",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/natemoo-re/proload.git",
    "directory": "packages/plugin-typescript"
  },
  "homepage": "https://github.com/natemoo-re/proload#readme",
